{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "error",
        "no-multiple-empty-lines": ["error", {
            "max": 2
        }],
        "indent": "off",
        "semi": "error",
        "import/prefer-default-export": "off",
        "no-tabs": "off",
        "import/extensions": "off",
        "import/order": "off",
        "import/no-unresolved": "off",
        "import/no-extraneous-dependencies": "off",
        "import/first": "off",
        "max-len": ["error", {"code": 120}],
        "camelcase": "off",
        "comma-dangle": "off",
        "import/no-duplicates": "off"

    }
}
